  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.3",
    "typescript": "5.8.2"
  },
  "workspaces": ["apps/*", "packages/*"],
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "author": {
    "name": "Justin Thomas",
